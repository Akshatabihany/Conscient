{
    "name": "Conscient",
    "version": "1.0",
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "content_scripts":[
        {
        "matches": ["<all_urls>"],
        "js": ["content.js"]

        }
    ],
    "browser_action":{
        "default_title":"conscient_extension",
        "default_popup":"popup.html"
    },

    "permissions": [
        "activeTab",
        "storage",
        "tabs"
]
}